            "attributes": {
                "ACCESSIBILITY": null,
                "FEEDESCRIPTION": null,
                "FORESTNAME": "Shawnee National Forest",
                "FORESTORGCODE": "0908",
                "LATITUDE": "37.724227",
                "LONGITUDE": "-88.537044",
                "MARKERACTIVITY": "Interpretive Areas",
