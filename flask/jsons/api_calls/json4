                "FORESTORGCODE": "0610",
                "LATITUDE": "42.693564",
                "LONGITUDE": "-124.039799",
                "MARKERACTIVITY": "Cabin Rentals",
                "MARKERACTIVITYGROUP": "Camping & Cabins",
                "OBJECTID": 4242,
